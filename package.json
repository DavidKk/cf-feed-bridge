{
  "name": "cf-feed-bridge",
  "version": "0.0.0",
  "description": "Transforms RSS feeds into import lists for various services, with API endpoints and local debugging support",
  "private": true,
  "scripts": {
    "commit": "git-cz",
    "format": "prettier --config .prettierrc.js --write \"**/*.{js,jsx,ts,tsx,d.ts,vue,md,json,yml,yaml}\"",
    "lint": "eslint --fix .",
    "test": "jest -c ./jest.config.ts --coverage --reporters=default --reporters=jest-junit",
    "test:watch": "jest --watch",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "cf-typegen": "wrangler types",
    "ok": "pnpm format && pnpm lint && pnpm test"
  },
  "dependencies": {
    "@cloudflare/workers-types": "latest",
    "fast-xml-parser": "^4.4.0",
    "itty-router": "^4.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@jest/types": "^29.6.3",
    "@types/jest": "^29.5.12",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "commitizen": "^4.3.0",
    "commitlint-plugin-i18n": "0.0.1-alpha.3",
    "cz-customizable": "^7.2.1",
    "cz-customizable-i18n-config": "0.0.1-alpha.2",
    "eslint": "^8.52.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "jest-runner-tsd": "^6.0.0",
    "json5": "^2.2.3",
    "lint-staged": "^15.2.9",
    "prettier": "^3.3.3",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "tsd-lite": "^0.9.0",
    "typescript": "^5.5.4",
    "wrangler": "^3.71.0"
  },
  "packageManager": "pnpm@9.7.1+sha512.faf344af2d6ca65c4c5c8c2224ea77a81a5e8859cbc4e06b1511ddce2f0151512431dd19e6aff31f2c6a8f5f2aced9bd2273e1fed7dd4de1868984059d2c4247"
